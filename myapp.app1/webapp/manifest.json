{
  "_version": "1.76.0",
  "sap.app": {
    "id": "myapp.app1.myapp.app1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.7",
      "toolsId": "71c802e4-f662-400a-88c3-d0d39d89b7b3"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/northwind/northwind.svc/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "myapp.app1.myapp.app1.i18n.i18n"
        }
      },
      "main": {
        "dataSource": "mainService",
        "type": "myapp.app1.myapp.app1.utils.odataLib",
        "preload": true,
        "settings": {}
      },
      "userModel": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "myapp.app1.myapp.app1.view",
        "async": true,
        "viewPath": "myapp.app1.myapp.app1.view"
      },
      "routes": [
        
        {
          "name": "OrderList",
          "pattern": "",
          "target": [
            "TargetOrderList"
          ]
        },
        {
          "name": "OrderDetails",
          "pattern": "OrderDetails/{orderId}",
          "target": [
            "TargetOrderDetails"
          ]
        }
      ],
      "targets": {
        "TargetOrderList" : {
          "id": "OrderList",
          "name": "OrderList"
        },
        "TargetOrderDetails" : {
          "id": "OrderDetails",
          "name": "OrderDetails",
          "viewLevel": 2
        }
      }
    },
    "rootView": {
      "viewName": "myapp.app1.myapp.app1.view.OrderList",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
